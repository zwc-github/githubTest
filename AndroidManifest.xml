<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.class_aug13_objanim"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.class_aug13_objanim.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.class_aug13_objanim.ObjAnimActivity"
            android:label="@string/title_activity_obj_anim" >
        </activity>
        <activity
            android:name="com.example.class_aug13_objanim.AnimSetActivity"
            android:label="@string/title_activity_anim_set" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.class_aug13_objanim.LayoutChangeActivity"
            android:label="@string/title_activity_layout_change" >
        </activity>
    </application>

</manifest>